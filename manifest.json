{
  "manifest_version": 3,
  "name": "威秀電影票選擇工具",
  "version": "1.0.0",
  "description": "協助使用者快速選擇威秀影城的電影場次資訊",
  "action": {
    "default_popup": "popup/popup.html"
  },
  "host_permissions": [
    "https://www.vscinemas.com.tw/*",
    "https://sales.vscinemas.com.tw/*"
  ],
  "permissions": [
    "cookies",
    "storage"
  ]
}
